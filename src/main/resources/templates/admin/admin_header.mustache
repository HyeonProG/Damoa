    <style type="text/css">
        body{
            padding: 0;
            margin: 0;
            width: 100%;
            height: 100vh;
            background:#d0ccc9;
        }
        header{
            width: 17%;
            height: 100%;
            padding-top: 30px;
            padding: 10px;
            box-shadow: 2px 2px 5px #dee2e6;
        background: white;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            text-align: center;
        }
        a{
            text-decoration: none;
        }
        hr{
            background: #d0ccc9;
            height:1px;
        }
        button{
            margin: 10px;
            padding: 7px;
            border: none;
            border-radius: 10px;
            color: white;
            background-color: #07a0c3
        }

        .main{
            width: 100%;
            height: 100%;
            display: flex;
            background:#f1f3f5;
        }
        .title-div{
            display: flex;
            align-items: center;
            background: white;
            height: 50px;
            box-shadow: 2px 2px 5px #dee2e6;
        }
        .main-board{
            width: 100%;
        }
        .main-box,.row{
            width: 95%;
            margin-top: 15px;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }
        .main-box{
            flex-direction: column;
        }
        .first-row-box{
            border: 1px solid #dee2e6;
            margin: 10px;
            border-radius: 5px;
            background: white;
            width: 30%;
            height: 30vh;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            text-align: center;
        }
        .second-row-box{
            border: 1px solid #dee2e6;
            margin: 10px;
            border-radius: 5px;
            background: white;
            width: 45%;
            height: 30vh;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            text-align: center;
        }
    </style>


